takim[name].sira=0
takim[masa2].sira=1
elseif r12[k] ~= -2 then
yer1=r12[k]
yerid1=2
r12[k]=-2
ui.updateTextArea(1011,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:yer1'>"..yer1,nill)
ui.updateTextArea(k,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..k.."'>X",name)
