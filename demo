end
if r33[k3] > 0 then
k34=r33[i]
r34[j3]=-4
r33[k3]=-3
r33[j3]=k34
r34[k3]=j34
ui.updateTextArea(k3,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..k3.."'>"..j34,name)
ui.updateTextArea(j3,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..j3.."'>"..k34,name)
print("1 ---------k5------")
end 
--- sarı son
end --- else son
takim[name].playeryer=0
else
j3=i
j31=r31[i]
j32=r32[i]
j33=r33[i]
j34=r34[i]
takim[name].playeryer=1
end
end --- tas3 fonk end

