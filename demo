function eventTextAreaCallback(id, name, callback) --- callback start

if callback=="a1" then
takim[name].takima=1
ui.removeTextArea(1001,nill)
ui.removeTextArea(1002,name)
ui.removeTextArea(1003,name)
ui.removeTextArea(1004,name)
ui.addTextArea(1006," ",nill,27,120,65,65,0x30BB34,0xF8FF00,0.2)
tfm.exec.movePlayer(name,60,150)
ui.addTextArea(1005," ",name,20,290,770,100,0x000000,0xF8FF00,500)
ui.addTextArea(0,"<b><font size='15' color='#2CFF00'><p align='center'><a href='event:baslat'>Oyunu Baslat</p></font>Not!: 4 kişi masaya oturmadan başlatırsanız hata alırsınız.",name,350,120,100,120,0x30BB34,0xF8FF00,0.2)

for i=1,26,1 do


randomcolor1=math.random(1,4)

if i<=14 then
if randomcolor1==1 then
sallama1=r1[math.random(#r1)]
table.insert(r11,i,sallama1)
removekey(r1,sallama1)
ui.addTextArea(i,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..i.."'>"..sallama1,name,x1,y1,45,45,0xFFFFFF,0xFFFFFF,500)
else
table.insert(r11,i,-1)
end
if randomcolor1==2 then
sallama1=r2[math.random(#r2)]
table.insert(r12,i,sallama1)
removekey(r2,sallama1)
ui.addTextArea(i,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..i.."'>"..sallama1,name,x1,y1,45,45,0xFFFFFF,0xFFFFFF,500)
else
table.insert(r12,i,-2)
end
if randomcolor1==3 then
sallama1=r3[math.random(#r3)]
table.insert(r13,i,sallama1)
removekey(r3,sallama1)
ui.addTextArea(i,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..i.."'>"..sallama1,name,x1,y1,45,45,0xFFFFFF,0xFFFFFF,500)
else
table.insert(r13,i,-3)
end
if randomcolor1==4 then
sallama1=r4[math.random(#r4)]
table.insert(r14,i,sallama1)
removekey(r4,sallama1)
ui.addTextArea(i,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..i.."'>"..sallama1,name,x1,y1,45,45,0xFFFFFF,0xFFFFFF,500)
else
table.insert(r14,i,-4)
end

else -- i>13
ui.addTextArea(i,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..i.."'>X",name,x1,y1,45,45,0xFFFFFF,0xFFFFFF,500)

table.insert(r11,i,-1)
table.insert(r12,i,-2)
table.insert(r13,i,-3)
table.insert(r14,i,-4)


end --i<=13



if y1<300 then
y1=345
else
y1=290
x1=x1+60
end
end



end --a1 end













