function tasdegis1(i,name) -- tas1 fonk bas
if takim[name].playeryer==1 then
k1=i

--- Siyahtan verme baş
if r11[j1] > 0 then 
if r11[k1] > 0 then
k11=r11[i]
r11[j1]=k11
r11[k1]=j11
ui.updateTextArea(k1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..k1.."'>"..j11,name)
ui.updateTextArea(j1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..j1.."'>"..k11,name)
print("1 ------s1---------")
end
if r11[k1] == -1 then
k11=r11[i]
r11[j1]=k11
r11[k1]=j11
ui.updateTextArea(k1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..k1.."'>"..j11,name)
ui.updateTextArea(j1,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..j1.."'>X",name)
print("1 -------s2--------")
end
if r12[k1] > 0 then
k11=r12[i]
r11[j1]=-1
r12[k1]=-2
r12[j1]=k11
r11[k1]=j11
ui.updateTextArea(k1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..k1.."'>"..j11,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..j1.."'>"..k11,name)
print("1 ----------s3-----")
end
if r13[k1] > 0 then
k11=r13[i]
r11[j1]=-1
r13[k1]=-3
r13[j1]=k11
r11[k1]=j11
ui.updateTextArea(k1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..k1.."'>"..j11,name)
ui.updateTextArea(j1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..j1.."'>"..k11,name)
print("1 -----------s4----")
end
if r14[k1] > 0 then
k11=r14[i]
r11[j1]=-1
r14[k1]=-4
r14[j1]=k11
r11[k1]=j11
ui.updateTextArea(k1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..k1.."'>"..j11,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..j1.."'>"..k11,name)
print("1 --------s5-------")
end 
--- Siyahtan verme son

--- Kırmızı verme baş
elseif r12[j1] > 0 then --- Siyahtan verme baş
if r12[k1] > 0 then
k12=r12[i]
r12[j1]=k12
r12[k1]=j12
ui.updateTextArea(k1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..k1.."'>"..j12,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..j1.."'>"..k12,name)
print("1 ----------kkk1-----")
end
if r12[k1] == -2 then
k12=r12[i]
r12[j1]=k12
r12[k1]=j12
ui.updateTextArea(k1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..k1.."'>"..j12,name)
ui.updateTextArea(j1,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..j1.."'>X",name)
print("1 -------k2--------")
end
if r11[k1] > 0 then
k12=r11[i]
r12[j1]=-2
r11[k1]=-1
r11[j1]=k12
r12[k1]=j12
ui.updateTextArea(k1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..k1.."'>"..j12,name)
ui.updateTextArea(j1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..j1.."'>"..k12,name)
print("1 --------k3-------")
end
if r13[k1] > 0 then
k12=r13[i]
r12[j1]=-2
r13[k1]=-3
r13[j1]=k12
r12[k1]=j12
ui.updateTextArea(k1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..k1.."'>"..j12,name)
ui.updateTextArea(j1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..j1.."'>"..k12,name)
print("1 -----------k4----")
end
if r14[k1] > 0 then
k12=r14[i]
r12[j1]=-2
r14[k1]=-4
r14[j1]=k12
r12[k1]=j12
ui.updateTextArea(k1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..k1.."'>"..j12,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..j1.."'>"..k12,name)
print("1 ---------k5------")
end --- Kırmızı verme son

--- yeşil verme baş
elseif r13[j1] > 0 then --- Siyahtan verme baş
if r13[k1] > 0 then
k13=r13[i]
r13[j1]=k13
r13[k1]=j13
ui.updateTextArea(k1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..k1.."'>"..j13,name)
ui.updateTextArea(j1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..j1.."'>"..k13,name)
print("1 ----------kkk1-----")
end
if r13[k1] == -3 then
k13=r13[i]
r13[j1]=k13
r13[k1]=j13
ui.updateTextArea(k1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..k1.."'>"..j13,name)
ui.updateTextArea(j1,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..j1.."'>X",name)
print("1 -------k2--------")
end
if r11[k1] > 0 then
k13=r11[i]
r13[j1]=-3
r11[k1]=-1
r11[j1]=k13
r13[k1]=j13
ui.updateTextArea(k1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..k1.."'>"..j13,name)
ui.updateTextArea(j1,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..j1.."'>"..k13,name)
print("1 --------k3-------")
end
if r12[k1] > 0 then
k13=r12[i]
r13[j1]=-3
r12[k1]=-2
r12[j1]=k13
r13[k1]=j13
ui.updateTextArea(k1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..k1.."'>"..j13,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..j1.."'>"..k13,name)
print("1 -----------k4----")
end
if r14[k1] > 0 then
k13=r14[i]
r13[j1]=-3
r14[k1]=-4
r14[j1]=k13
r13[k1]=j13
ui.updateTextArea(k1,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..k1.."'>"..j13,name)
ui.updateTextArea(j1,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..j1.."'>"..k13,name)
print("1 ---------k5------")
end 
