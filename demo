if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
r13[t]=yer4
ui.updateTextArea(t,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..t.."'>"..yer4,name)
break
end
end
elseif yerid4==4 then
for t=1,26,1 do
if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
r14[t]=yer4
ui.updateTextArea(t,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..t.."'>"..yer4,name)
break
end
end
end
ui.addTextArea(1014,"<b><font size='25' color='#FFFFFF'>X",nill,150,80,45,45,0xFFFFFF,0xFFFFFF,500)
takim[name].sira=2
end
if callback=="tascek" then
tasrenkrandom1=math.random(1,4)
if tasrenkrandom1==1 then
tas11=r1[math.random(#r1)]
removekey(r1,tas11)
for t=1,26,1 do
if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..t.."'>"..tas11,name)
r11[t]=tas11
break
end
end
elseif tasrenkrandom1==2 then
tas12=r2[math.random(#r2)]
removekey(r2,tas12)
for t=1,26,1 do
if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..t.."'>"..tas12,name)
r12[t]=tas12
break
end
end
elseif tasrenkrandom1==3 then
tas13=r3[math.random(#r3)]
removekey(r3,tas13)
for t=1,26,1 do
if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..t.."'>"..tas13,name)
r13[t]=tas13
break
end
end
elseif tasrenkrandom1==4 then
tas14=r4[math.random(#r4)]
removekey(r4,tas14)
for t=1,26,1 do
if r11[t] == -1 and r12[t] == -2  and r13[t] == -3  and r14[t] == -4 then
