function removekey(rx,sayi)
for anahtar,deger in ipairs(rx) do

if deger==sayi then
table.remove(rx,anahtar)
--print(sayi)
break
end

end
end

function eventNewPlayer(name)
takim[name]={takima=0,playeryer=0,ptas1=0}

end
for name in pairs (tfm.get.room.playerList) do
	eventNewPlayer(name)
end

