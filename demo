function eventTextAreaCallback(id, name, callback) --- callback start
if callback=="okey" then --- okey
if takim[name].takima==1 then  --- takim 1
print(name)
end --- takim 1
end --- okey
if takim[name].takima==4 then --- takim 4 end
if takim[name].sira==0 then
for i=1,26,1 do
if callback=="x"..i then
tasdegis4(i,name)
end
end
elseif takim[name].sira==1 then
if callback=="yer3" then
if yerid3==1 then
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
r41[t]=yer3

ui.updateTextArea(t,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..t.."'>"..yer3,name)
break
end
end
elseif yerid3==2 then
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
r42[t]=yer3
ui.updateTextArea(t,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..t.."'>"..yer3,name)
break
end
end
elseif yerid3==3 then
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
r43[t]=yer3
ui.updateTextArea(t,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..t.."'>"..yer3,name)
break
end
end
elseif yerid3==4 then
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
r44[t]=yer3
ui.updateTextArea(t,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..t.."'>"..yer3,name)
break
end
end
end
ui.updateTextArea(1013,"<b><font size='25' color='#FFFFFF'>X",nill)
takim[name].sira=2
end
if callback=="tascek" then
tasrenkrandom4=math.random(1,4)
if tasrenkrandom4==1 then
tas41=r1[math.random(#r1)]
removekey(r1,tas41)
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:x"..t.."'>"..tas41,name)
r41[t]=tas41
break
end
end
elseif tasrenkrandom4==2 then
tas42=r2[math.random(#r2)]
removekey(r2,tas42)
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:x"..t.."'>"..tas42,name)
r42[t]=tas42
break
end
end
elseif tasrenkrandom4==3 then
tas43=r3[math.random(#r3)]
removekey(r3,tas43)
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:x"..t.."'>"..tas43,name)
r43[t]=tas43
break
end
end
elseif tasrenkrandom4==4 then
tas44=r4[math.random(#r4)]
removekey(r4,tas44)
for t=1,26,1 do
if r41[t] == -1 and r42[t] == -2  and r43[t] == -3  and r44[t] == -4 then
ui.updateTextArea(t,"<b><font size='25' color='#BABB30'><p align='center'><a href='event:x"..t.."'>"..tas44,name)
r44[t]=tas44
break
end
end
end
takim[name].sira=2
end -- tascek
elseif takim[name].sira==2 then
for k=1,26,1 do
if callback=="x"..k then
if r41[k] ~= -1 then
yer4=r41[k]
yerid4=1
r41[k]=-1
ui.updateTextArea(1014,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:yer4'>"..yer4,nill)
ui.updateTextArea(k,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..k.."'>X",name)
takim[name].sira=0
takim[masa1].sira=1
elseif r42[k] ~= -2 then
yer4=r42[k]
yerid4=2
r42[k]=-2
ui.updateTextArea(1014,"<b><font size='25' color='#BB3030'><p align='center'><a href='event:yer4'>"..yer4,nill)
ui.updateTextArea(k,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..k.."'>X",name)
takim[name].sira=0
takim[masa1].sira=1
elseif r43[k] ~= -3 then
yer4=r43[k]
yerid4=3
r43[k]=-3
ui.updateTextArea(1014,"<b><font size='25' color='#38BB30'><p align='center'><a href='event:yer4'>"..yer4,nill)
ui.updateTextArea(k,"<b><font size='25' color='#FFFFFF'><p align='center'><a href='event:x"..k.."'>X",name)
takim[name].sira=0
