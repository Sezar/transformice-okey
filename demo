function eventChatMessage(name,message)
if message=="si1" then
for anahtar,deger in ipairs(r11) do -- tabloyu döndürdük (bize her elemanın sırasını ve değerini değişken olarak verir)
print(anahtar.." |siyah "..deger) -- verilen anahtarı ve değeri yazdırdık
end
end

if message=="ki1" then
for anahtar,deger in ipairs(r12) do -- tabloyu döndürdük (bize her elemanın sırasını ve değerini değişken olarak verir)
print(anahtar.." |k "..deger) -- verilen anahtarı ve değeri yazdırdık
end
end

if message=="ye1" then
for anahtar,deger in ipairs(r13) do -- tabloyu döndürdük (bize her elemanın sırasını ve değerini değişken olarak verir)
print(anahtar.." |y "..deger) -- verilen anahtarı ve değeri yazdırdık
end
end

if message=="sa1" then
for anahtar,deger in ipairs(r14) do -- tabloyu döndürdük (bize her elemanın sırasını ve değerini değişken olarak verir)
print(anahtar.." |sari "..deger) -- verilen anahtarı ve değeri yazdırdık
end
end


end













