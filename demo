takim[masa2].sira=1
end
ui.addTextArea(1006," ",nill,27,120,65,65,0x000000,0xF8FF00,0.2) --- 1
ui.addTextArea(1008,"",nill,360,220,65,65,0x30BB34,0xF8FF00,0.2) --- 2
ui.addTextArea(1009," ",nill,707,120,65,65,0x000000,0xF8FF00,0.2) --- 3
ui.addTextArea(1007," ",nill,360,20,65,65,0x000000,0xF8FF00,0.2) --- 4
end
end
end
end --- takim 1 end
