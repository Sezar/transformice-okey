r14[t]=tas14
break
end
end
end
takim[name].sira=2
end -- tascek
elseif takim[name].sira==2 then
for k=1,26,1 do
if callback=="x"..k then
if r11[k] ~= -1 then
yer1=r11[k]
yerid1=1
r11[k]=-1
ui.updateTextArea(1011,"<b><font size='25' color='#7F7F7F'><p align='center'><a href='event:yer1'>"..yer1,nill)
